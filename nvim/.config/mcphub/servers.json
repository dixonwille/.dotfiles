{
  "mcpServers": {
    "fetch": {
      "args": [
        "mcp-server-fetch"
      ],
      "command": "uvx"
    },
    "filesystem": {
      "args": [
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "${env:PROJECT_ROOT}"
      ],
      "command": "npx",
      "disabled": false
    },
    "git": {
      "args": [
        "mcp-server-git",
        "--repository",
        "${env:PROJECT_ROOT}"
      ],
      "command": "uvx",
      "disabled": false
    },
    "sequentialthinking": {
      "args": [
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ],
      "command": "npx"
    }
  },
  "nativeMCPServers": {
    "mcphub": {
      "disabled": false
    }
  }
}
